    "def mutate_string(string, position, character):\n",
    "    s = list(string)\n",
    "    s[position] = character\n",
    "    string = \"\".join(s)\n",
    "    return string\n",
    "\n",
    "s = \"qwerty\"\n",
    "position = 5\n",
    "character = \"i\"\n",
    "mutate_string(s, position, character)"
